#!/usr/bin/env sh

# Maven Wrapper script for Unix-based systems
# Use this script to run Maven commands without requiring Maven to be installed on your system.

MAVEN_VERSION=3.8.6
MVNW_HOME="$(cd "$(dirname "$0")/.."; pwd)/.mvn"

if [ ! -d "$MVNW_HOME" ]; then
  echo "Maven wrapper not found. Please run 'mvn -N io.takari:maven:wrapper' to generate it."
  exit 1
fi

# Execute Maven with the specified version
exec "$MVNW_HOME/wrapper/maven-wrapper.jar" "$@"